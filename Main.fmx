                DataDetectorTypes = []
                Lines.Strings = (
                  
                    '//$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$' +
                    '$$$$$$$$$$$$$$$$'#12304#23450#25968#12305
                  ''
                  'static const float Pi = 3.141592653589793;'
                  ''
                  'static const float Pi2 = Pi * 2.0;'
                  ''
                  'static const float P2i = Pi / 2.0;'
                  ''
                  
                    '//$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$' +
                    '$$$$$$$$$$$$$$$$'#12304#35373#23450#12305
                  ''
                  'SamplerState _SamplerState {};'
                  ''
                  
                    '//$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$' +
                    '$$$$$$$$$$$$$$$$'#12304#12523#12540#12481#12531#12305
                  ''
                  'float Pow2( float X_ )'
                  '{'
                  '    return X_ * X_;'
                  '}'
                  ''
                  'float Roo2( float X_ )'
                  '{'
                  '    return sqrt( X_ );'
                  '}'
                  ''
                  'float2 VectorToSky( float3 Vector_ )'
                  '{'
                  '    float2 _Result;'
                  ''
                  '    _Result.x = ( Pi - atan2( -Vector_.z, -Vector_.x ) ) / Pi2;'
                  '    _Result.y =        acos ( -Vector_.y             )   / Pi ;'
                  ''
                  '    return _Result;'
                  '}'
                  ''
                  'float Fresnel( float3 EyeVec_, float3 NorVec_, float RefI_ )'
                  '{'
                  '    float N = Pow2( RefI_ );'
                  '    float C = dot( EyeVec_, NorVec_ );'
                  '    float G = sqrt( N + Pow2( C ) - 1 );'
                  '    float NC = N * C;'
                  '    return ( Pow2( (  C - G ) / (  C + G ) )'
                  '           + Pow2( ( NC - G ) / ( NC + G ) ) ) / 2;'
                  ''
                  '    /* '#36817#20284
                  '    float R = pow( ( RefI_ - 1 ) / ( RefI_ + 1 ), 2 );'
                  '    float C = dot( EyeVec_, NorVec_ );'
                  '    return R + ( 1 - R ) * pow( 1 - C, 5 );'
                  '    */'
                  '}'
                  ''
                  
                    '//##############################################################' +
                    '################'
                  ''
                  'struct TSenderP               //'#12501#12521#12464#12513#12531#12488#12398#22793#25968#22411
                  '{'
                  '    float4 Scr :SV_Position;  //'#20301#32622#65288#12473#12463#12522#12540#12531#65289
                  '    float4 Pos :TEXCOORD0  ;  //'#20301#32622#65288#12464#12525#12540#12496#12523#65289
                  '    float4 Tan :TANGENT    ;  //'#25509#32218#65288#12464#12525#12540#12496#12523#65289
                  '    float4 Bin :BINORMAL   ;  //'#24467#27861#32218#65288#12464#12525#12540#12496#12523#65289
                  '    float4 Nor :NORMAL     ;  //'#27861#32218#65288#12464#12525#12540#12496#12523#65289
                  '    float4 Tex :TEXCOORD1  ;  //'#12486#12463#12473#12481#12515#24231#27161
                  '};'
                  ''
                  'struct TResultP               //'#12500#12463#12475#12523#12398#22793#25968#22411
                  '{'
                  '    float4 Col :SV_Target  ;  //'#33394
                  '};'
                  ''
                  
                    '////////////////////////////////////////////////////////////////' +
                    '////////////////'
                  ''
                  'TResultP MainP( TSenderP _Sender )'
                  '{'
                  '    TResultP _Result;'
                  ''
                  
                    '    float3 N = normalize( _Sender.Nor.xyz );                    ' +
                    '                //'#34920#38754#27861#32218#65288#12464#12525#12540#12496#12523#65289
                  
                    '    float3 T = normalize( _Sender.Tan.xyz );                    ' +
                    '                //'#34920#38754#25509#32218#65288#12464#12525#12540#12496#12523#65289
                  
                    '    float3 B = normalize( _Sender.Bin.xyz );                    ' +
                    '                //'#34920#38754#24467#27861#32218#65288#12464#12525#12540#12496#12523#65289
                  
                    '    float3 L = -_Light.Dir.xyz;                                 ' +
                    '                //'#20809#32218#26041#21521#65288#12464#12525#12540#12496#12523#65289
                  
                    '    float3 V = normalize( _EyePos.xyz - _Sender.Pos.xyz );      ' +
                    '                //'#35222#32218#26041#21521#65288#12464#12525#12540#12496#12523#65289
                  
                    '    float3 H = normalize( L + V );                              ' +
                    '                //'#12495#12540#12501#12505#12463#12488#12523
                  ''
                  
                    '    //----------------------------------------------------------' +
                    '----------------'
                  ''
                  
                    '    if ( dot( N, V ) < 0 ) N = -N;                              ' +
                    '                //'#35023#38754#12398#27861#32218#12434#21453#36578
                  ''
                  
                    '    clip( -_Sender.Pos.z );                                     ' +
                    '                //'#21322#29699#12398#25551#30011#12434#12461#12515#12531#12475#12523
                  ''
                  
                    '    //----------------------------------------------------------' +
                    '----------------'
                  ''
                  
                    '    float LD = max( dot( N, L ), 0.0 );                         ' +
                    '                //'#20809#32218#25313#25955#21453#23556#29575
                  ''
                  
                    '    float4 D = _DiffImage.Sample( _SamplerState, _Sender.Tex.xy ' +
                    ');              //'#25313#25955#21453#23556#29575#12510#12483#12503#33394
                  ''
                  
                    '    float4 CD = ( _AmbiLight + _Light.Col * LD ) * ( _DiffRatio ' +
                    '* D );          //'#25313#25955#21453#23556#20809
                  ''
                  
                    '    //----------------------------------------------------------' +
                    '----------------'
                  ''
                  
                    '    float LS = pow( max( dot( N, H ), 0.0 ), _SpecShiny );      ' +
                    '                //'#20809#32218#37857#38754#21453#23556#29575
                  ''
                  
                    '    float4 CS = ( _Light.Col * LS ) * _SpecRatio;               ' +
                    '                //'#20809#32218#37857#38754#21453#23556#20809
                  ''
                  
                    '    //----------------------------------------------------------' +
                    '----------------'
                  ''
                  '    _Result.Col = _EmisLight + CD + CS;'
                  ''
                  
                    '    //----------------------------------------------------------' +
                    '----------------'
                  ''
                  '    _Result.Col.a = 1;'
                  ''
                  '    _Result.Col = _Opacity * _Result.Col;'
                  ''
                  '    return _Result;'
                  '}'
                  ''
                  
